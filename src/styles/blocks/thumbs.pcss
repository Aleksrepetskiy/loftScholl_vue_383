.thumbs {
	position: relative;
	display: flex;
	justify-content: flex-end;
	&__link {
		display: block;
		height: 100%;
		width: 100%;
	}
	&__item {
		width: 19%;
		height: 106px;
		
		transition: .2s;
		cursor: pointer;
		&.active,
		&:hover {
			outline: 3px solid #f29400;
			z-index: 1;
		}
	}

}
.thumb {
	&__pic {
		object-fit: cover;
		width: 100%;
		height: 100%;
	}

}